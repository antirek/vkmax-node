{
  "name": "vkmax-node",
  "version": "1.0.0",
  "description": "Node.js client for VK MAX messenger (OneMe)",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "example": "node dist/examples/weather-userbot.js",
    "example:simple": "node dist/examples/simple-example.js",
    "example:media": "node dist/examples/media-upload-example.js",
    "example:photo": "node dist/examples/send-photo-example.js",
    "example:vasiliy": "node dist/examples/send-photo-to-vasiliy.js",
    "example:realphoto": "node dist/examples/send-real-photo.js",
    "test:upload": "node dist/examples/test-upload-flow.js",
    "test:opcode65": "node dist/examples/test-opcode65.js",
    "test:opcode80": "node dist/examples/test-opcode80.js",
    "send:photo": "node dist/examples/send-full-photo.js",
    "test:simple": "node dist/examples/test-simple-upload.js",
    "test:fields": "node dist/examples/test-field-names.js",
    "test:jpeg": "node dist/examples/test-jpeg-upload.js",
    "test:native": "node dist/examples/test-native-formdata.js",
    "send:real": "node dist/examples/send-real-photo-attachment.js",
    "send:working": "node dist/examples/send-working-photo.js",
    "example:upload": "node dist/examples/upload-photo-step-by-step.js",
    "example:simple-photo": "node dist/examples/simple-photo-upload.js",
    "docs": "typedoc",
    "docs:serve": "typedoc --watch"
  },
  "keywords": [
    "vk",
    "max",
    "oneme",
    "messenger",
    "websocket",
    "client"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "form-data": "^4.0.0",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.0",
    "typedoc": "^0.28.10",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
